---
layout: layouts/base.njk
---
<article class="content__article">
  <header class="content__header">
    <h1>{{ title }}</h1>
    <div class="content__meta">
      <time datetime="{{ date | w3DateFilter }}" class="dt-published">{{ date | date("YYYY-MM-DD") }}</time>
    </div>
  </header>
    {{ content | safe }}
</article>
